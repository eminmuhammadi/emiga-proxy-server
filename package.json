    "fs": "0.0.1-security",
    "helmet": "^3.20.0",
    "http": "0.0.0",
    "http-proxy": "^1.17.0",
    "http-proxy-middleware": "^0.19.1",
    "https": "^1.0.0",
    "tsscmp": "^1.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eminmuhammadi/emiga-proxy-server.git"
  },
