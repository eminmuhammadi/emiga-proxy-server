{
  "name": "emiga-proxy-server",
  "version": "0.0.1",
  "description": "Full customizeable proxy server",
  "main": "server/proxy.js",
  "scripts": {
    "start": "cd server && node proxy.js",
    "proxy": "cd server && nodemon proxy.js",
